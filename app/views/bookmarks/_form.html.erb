<%= render partial: 'layouts/errors', locals: {object: @bookmark} %>

<%= form_with model: @bookmark do |f| %>
    <%= f.label :select_tag_for_bookmark %>
    <%= f.collection_select :tag_id, Tag.all, :id, :name %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :url %>
    <%= f.url_field :url %>

    <%= f.submit %>
  <% end %>
