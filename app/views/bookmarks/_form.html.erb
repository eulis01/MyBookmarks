<%= render partial: 'layouts/errors', locals: {object: @bookmark} %>

<%= form_with model: @bookmark do |f| %>
      <%= f.label :name, 'NAME:' %>
      <%= f.text_field :name %> <br><br>
      <%= f.label :url, 'URL:' %>
      <%= f.url_field :url %><br><br>

            <%= f.fields_for :tags do |t| %>
                  <%= t.label :select_tag, 'Select A Tag:' %>
                  <%= t.collection_select :id, Tag.all, :id, :name, include_blank: true %><br><br>
                  <%= t.label :name, 'New Tag Name:' %>
                  <%= t.text_field :name %><br><br>
            <% end %>
<%= f.submit %>
<% end %>
