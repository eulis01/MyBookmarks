<%= render partial: 'layouts/errors', locals: {object: @bookmark} %>

<%= form_with model: @bookmark do |f| %>
    <%= f.label :select_tag, 'Select A Tag:' %>
    <%= f.collection_select :tag_id, Tag.all, :id, :name %><br><br>
    <%= f.label :name, 'Name:' %>
    <%= f.text_field :name %><br><br>
    <%= f.label :url, 'URL:' %>
    <%= f.url_field :url %><br><br>
    <%= f.submit %>
  <% end %>
