<h1> All Your Bookmarks </h1>
<tr>
  <% @bookmarks.each do |b|%>
      <li>
        <a href="<%= b.url %>" target="_blank" rel="noopener"> <%= b.name %>  </a> --
        <%= link_to "Show", bookmark_path(b) %> --
        <%= link_to "Edit", edit_bookmark_path(b) %> --
        <%= link_to "Delete", bookmark_path(b), method: :delete, data: { confirm: "Are you sure?" } %>
      </li>
  <% end %>
</tr>
